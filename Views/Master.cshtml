@inherits Umbraco.Web.Mvc.UmbracoViewPage

@using ClientDependency.Core.Mvc

@{
	Layout = null;

	Html.RequiresCss("https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700");
	Html.RequiresCss("/assets/lib/bootstrap/css/bootstrap.min.css");
	Html.RequiresCss("/assets/lib/font-awesome/css/font-awesome.min.css");
	Html.RequiresCss("/assets/lib/animate/animate.min.css");
	Html.RequiresCss("/assets/lib/ionicons/css/ionicons.min.css");
	Html.RequiresCss("/assets/lib/owlcarousel/assets/owl.carousel.min.css");
	Html.RequiresCss("/assets/lib/freecss/style.css");

	Html.RequiresJs("/assets/lib/jquery/jquery.min.js");
	Html.RequiresJs("/assets/lib/jquery/jquery-migrate.min.js");
	Html.RequiresJs("/assets/lib/popper/popper.min.js");
	Html.RequiresJs("/assets/lib/bootstrap/js/bootstrap.min.js");
	Html.RequiresJs("/assets/lib/easing/easing.min.js");
	Html.RequiresJs("/assets/lib/owlcarousel/owl.carousel.min.js");
	Html.RequiresJs("/assets/lib/scrollreveal/scrollreveal.min.js");
	Html.RequiresJs("/assets/lib/freecss/main.js");
}

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title>Umbraco Estate Agency</title>

	@Html.RenderCssHere()
</head>

<body>

	@Html.Partial("~/Views/Partials/Navigation.cshtml")

	<div>
		@RenderBody()
	</div>

	@Html.RenderJsHere()
</body>